function autogetmiddle()
    while getgenv().GetMiddleblocks == true do
        for _,v in pairs(game:GetService("Workspace").CenterBlocks.Givers:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch
end
end
end
end



function AutoTpbase()
        
        local lowhealth = false
local Humanoid= game.Players.LocalPlayer.Character.Humanoid
Humanoid.Changed:connect(function()
    if Humanoid.Health <= 40 and not lowhealth then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1039.5282, -128.299408, 85.450943, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        lowhealth = true
    elseif Humanoid.Health > 40 then
        lowhealth = false
    end
end)
end



