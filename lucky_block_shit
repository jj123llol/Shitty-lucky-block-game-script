local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "lucky block--by jude/rev", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


 


local Tab = Window:MakeTab({
	Name = "Script stuff",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
getgenv().GetMiddleblocks = true;
getgenv().TeleportBase = true;

Tab:AddToggle({
	Name = "Auto get blocks from middle",
	Default = (bool),
	Callback = function(bool)
		getgenv().GetMiddleblocks = bool
		print(bool)
		if bool then
		autogetmiddle()
		end
	end    
    
})

Tab:AddButton({
	Name = "Get blocks from middle",
	Callback = function()
      		for _,v in pairs(game:GetService("Workspace").CenterBlocks.Givers:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch
end
end
end
})


