Tab:AddParagraph("Credits","Credits to Amnoob for 3 lines of coding")




local lowhealth = false
local Humanoid= game.Players.LocalPlayer.Character.Humanoid
Humanoid.Changed:connect(function()
    if getgenv().TeleportBase then
        if Humanoid.Health <= 40 and not lowhealth then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1039.5282, -128.299408, 85.450943, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            lowhealth = true
        elseif Humanoid.Health > 40 then
            lowhealth = false
        end
    end
end)
local lowhealth = false
local Humanoid= game.Players.LocalPlayer.Character.Humanoid
Humanoid.Changed:connect(function()
    if getgenv().TeleportBase then
        if Humanoid.Health <= 40 and not lowhealth then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1039.5282, -128.299408, 85.450943, 1, 0, 0, 0, 1, 0, 0, 0, 1)
            lowhealth = true
        elseif Humanoid.Health > 40 then
            lowhealth = false
        end
    end
end)
